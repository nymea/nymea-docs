{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "tools/fix-svelte-compiler",
    "dev": "NODE_ENV=dev run-p routify rollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "NODE_ENV=production routify -bD && rollup -c",
    "serve": "NODE_ENV=production spassr --serve-spa --serve-ssr",
    "export": "NODE_ENV=production routify export",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D"
  },
  "devDependencies": {
    "@github-docs/frontmatter": "^1.3.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "mdsvex": "^0.8.7",
    "npm-run-all": "^4.1.5",
    "ppid-changed": "^1.0.1",
    "rollup": "^2.6.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-image-base64": "^1.0.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "spassr": "^1.0.2",
    "svelte-check": "^1.0.19",
    "svelte-image": "^0.2.7",
    "svelte-preprocess": "^4.0.12",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@beyonk/gdpr-cookie-consent-banner": "^6.4.1",
    "@sveltech/routify": "^1.9.9",
    "@sveltejs/svelte-scroller": "^2.0.6",
    "clipboard": "^2.0.6",
    "highlight.js": "^10.0.3",
    "markdown-it-replace-link": "^1.0.1",
    "svelte": "^3.23.0",
    "svelte-waypoint": "^0.1.4"
  }
}
